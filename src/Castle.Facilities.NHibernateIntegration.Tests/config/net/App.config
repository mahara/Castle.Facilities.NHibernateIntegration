<?xml version="1.0"?>

<configuration>

  <!--
    Version for .NET.

    The `<system.diagnostics>` section is not preregistered,
    and must be declared in `<configSections>` before use.
  -->

  <configSections>
    <section name="system.diagnostics" type="System.Diagnostics.DiagnosticsConfigurationHandler" />
  </configSections>

  <appSettings>
    <add key="nhf.connection.provider"
         value="NHibernate.Connection.DriverConnectionProvider" />
    <add key="nhf.connection.driver_class"
         value="NHibernate.Driver.Sql2008ClientDriver" />
    <add key="nhf.dialect"
         value="NHibernate.Dialect.MsSql2008Dialect" />
    <add key="nhf.connection.connection_string.1"
         value="Server=.; Initial Catalog=test; Integrated Security=SSPI" />
    <add key="nhf.connection.connection_string.2"
         value="Server=.; Initial Catalog=test2; Integrated Security=SSPI" />
  </appSettings>

  <system.diagnostics>
  </system.diagnostics>

</configuration>